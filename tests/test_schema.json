{
    "openapi": "3.0.0",
    "info": {
        "title": "Example API",
        "version": "1.0.0"
    },
    "paths": {
        "/example": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Example response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ExampleObject"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ExampleObject": {
                "type": "object",
                "properties": {
                    "id": {
                        "$ref": "#/components/schemas/Id"
                    },
                    "name": {
                        "type": "string"
                    },
                    "isActive": {
                        "type": "boolean"
                    },
                    "details": {
                        "$ref": "#/components/schemas/Details"
                    }
                },
                "required": [
                    "id",
                    "name"
                ]
            },
            "Id": {
                "type": "integer",
                "format": "int64"
            },
            "Details": {
                "type": "object",
                "properties": {
                    "description": {
                        "type": "string"
                    },
                    "count": {
                        "$ref": "#/components/schemas/Count"
                    }
                },
                "required": [
                    "description"
                ]
            },
            "Count": {
                "type": "integer"
            }
        }
    }
}